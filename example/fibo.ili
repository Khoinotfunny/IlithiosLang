# fibonacci_loop.ili - Cách vòng lặp
func fib_loop(n) {
    a = 0;
    b = 1;
    i = 0;
    
    while (i < n) {
        temp = a;
        a = b;
        b = temp + b;
        i = i + 1;
    }
    return a;
}

print "Day Fibonacci tu 0 den 10:";
count = 0;
while (count <= 10) {
    print fib_loop(count);
    count = count + 1;
}